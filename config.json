          "permission": "VOTE__ADMIN_MANAGE_REWARDS",
          "route": "/admin/vote/rewards"
        },
        "Gérer les sites de votes": {
          "icon": "sitemap",
          "permission": "VOTE__ADMIN_MANAGE_WEBSITES",
          "route": "/admin/vote/websites"
        },
        "Meilleurs voteurs": {
          "icon": "trophy",
          "permission": "VOTE__ADMIN_VIEW_TOP",
          "route": "/admin/vote/top"
        }
      }
    }
  },
  "navbar_routes": {
    "Voter": "/vote"
  },
  "author": "Eywek",
  "version": "2.4.1",
  "useEvents": true,
  "permissions": {
